{
  "name": "react-parcel-app",
  "version": "1.0.0",
  "description": "RL",
  "main": "src/App.js",
  "scripts": {
    "build": "parcel build --public-url ./dist/ src/index.html",
    "start": "npm -s run build && babel-node server/server.js",
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock npm run dev",
    "format": "prettier --write \"src/**/*.{js,jsx,html}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:update": "jest -u"
  },
  "keywords": [],
  "author": "Rocky L",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.2.1",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
